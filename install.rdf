<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">

    <em:bootstrap>true</em:bootstrap>
    <em:type>2</em:type>
    <em:id>firefox-stash@glob.com.au</em:id>
    <em:name>Stash</em:name>
    <em:version>1.0.1</em:version>
    <em:description>Allows a window to be 'stashed' (bookmarked and closed) and later 'restored' (reopened and bookmarks deleted).</em:description>
    <em:creator>Byron Jones</em:creator>

    <!-- Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>3.6</em:minVersion>
        <em:maxVersion>4.0.*</em:maxVersion>
      </Description>
    </em:targetApplication>

  </Description>

</RDF>
